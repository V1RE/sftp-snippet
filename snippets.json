{
  "SFTP Template": {
    "prefix": "sftp",
    "body": [
      "{",
      "\t\"name\": \"${1:domain}\",",
      "\t\"host\": \"${1:domain}.sftp.wpengine.com\",",
      "\t\"protocol\": \"sftp\",",
      "\t\"port\": 2222,",
      "\t\"username\": \"${1:domain}-${2:user}\",",
      "\t\"password\": \"$0\",",
      "\t\"remotePath\": \"/\",",
      "\t\"uploadOnSave\": true,",
      "\t\"watcher\": {",
      "\t\t\"files\": \"**/*.{css,js,ts,scss,php,html,json}\",",
      "\t\t\"autoUpload\": false,",
      "\t\t\"autoDelete\": false",
      "\t},",
      "\t\"ignore\": [",
      "\t\t\".vscode\",",
      "\t\t\".git\",",
      "\t\t\".DS_Store\",",
      "\t\t\"node_modules/\"",
      "\t]",
      "}"
    ]
  }
}
